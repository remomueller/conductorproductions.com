# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Remove
/config/database.yml

/public/assets/*
/public/cache/*
/public/stylesheets/cache/*
/public/javascripts/cache/*
/public/20*

# Other Files

/Gemfile.lock
/config/secrets.yml
/db/schema.rb
/doc/app/*

/.sass-cache/
/coverage/*

# OS generated files #
.DS_Store

# Specific Conductor Productions Ignore Folders
/app/assets/fonts/
/app/assets/layers/
/app/assets/images/videos/drtv/*.jpg
/app/assets/images/videos/work/*.jpg
