                <div class="video-outer">
                    <div class="video-container">
                        <span class="play-btn" title="Play">
                            <span class="play"></span>
                        </span>
                        <video id="standalonePlayer" preload="true" class="video" src="https://player.vimeo.com/external/124955438.hd.mp4?s=a2467bbea9b3f59360e89a344e400803e5790d3d&profile_id=119" poster="/images/directors/vladimir/posters/featured-video.png">
